* {
  background-color: #282C3388;
  border-color: #2e343f;
  text-color: #8ca0aa;
  spacing: 0;
  width: 512px;
  font: "Ubuntu 14";
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
  border: 0 1px 0 0;
  font: "Ubuntu Bold 14";
}

textbox {
  background-color: #2e343f;
  border: 0 0 1px 0;
  border-color: #282C33;
  padding: 8px 16px;
}

entry {
  padding: 16px;
  font: "Ubuntu Bold 14";
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
  lines: 5;
}

element {
  border: 0 0 1px 0;
  padding: 16px;
}

element selected {
  background-color: #0f67c688;
}
